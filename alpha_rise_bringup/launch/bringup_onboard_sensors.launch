<launch>

  <arg name="robot_name" default="alpha_rise"/>

  <node ns="$(arg robot_name)" pkg="power_monitor" type="power_monitor_node" 
        name="power_monitor_node" output="screen">
      <rosparam command="load" file="$(find alpha_rise_config)/config/power_minitor.yaml"/>
      <param name="frame_id" value="$(arg robot_name)/power_minitor"/>
  </node>

  <include file="$(find alpha_rise_bringup)/launch/include/xsens_imu.launch.xml">
    <arg name="robot_name" value="$(arg robot_name)"/>
  </include> 

  <node ns="$(arg robot_name)" pkg="bluerobotics_pressure" type="driver.py" 
      name="bluerobotics_pressure_node" output="screen">
  <rosparam command="load" file="$(find alpha_rise_config)/config/bluerobotcs_bar30.yaml"/>
  <param name="frame_id" value="$(arg robot_name)/pressure"/>
  </node>

  
</launch>
